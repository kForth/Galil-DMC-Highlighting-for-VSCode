{
    "name": "galildmchighlighting",
    "displayName": "GalilDmcHighlighting",
    "description": "Syntax Highlighting for the Galil DMC Language",
    "version": "0.1.0",
    "publisher": "kestin",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dmc",
            "aliases": ["Galil DMC", "dmc"],
            "extensions": [".dmc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dmc",
            "scopeName": "source.dmc",
            "path": "./syntaxes/dmc.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kForth/Galil-DMC-Highlighting-for-VSCode.git"
    }
}